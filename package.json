    "copy-static": "cpy \"public/**/*\" dist"
  },
  "devDependencies": {
    "cpy-cli": "^6.0.0",
    "esbuild": "^0.25.9",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  }
}
